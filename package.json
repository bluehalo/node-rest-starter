{
  "name": "node-rest-starter",
  "description": "Starter for a REST API using Node and Express.js",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com:Asymmetrik/node-rest-starter.git"
  },
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "nodemon ./src/server.js --exec 'npm run lint && node --inspect' | bunyan",
    "start:dev": "export NODE_ENV=development && npm run start",
    "test": "NODE_ENV=test nodemon ./src/test.js",
    "test:ci": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary node ./src/test.js --ci",
    "test:gitlab": "NODE_ENV=testgitlab nyc --reporter=lcov --reporter=text-summary node ./src/test.js --ci",
    "test:debug": "NODE_ENV=test nodemon ./src/test.js --exec 'node --inspect-brk=:9339'",
    "build": "npm run lint",
    "lint": "npm run lint:eslint",
    "lint:fix": "npm run lint:eslint:fix",
    "lint:eslint": "eslint \"./**/*.js\"",
    "lint:eslint:fix": "eslint \"./**/*.js\" --fix"
  },
  "nyc": {
    "include": "src",
    "exclude": [
      "**/*.spec.js",
      "src/test.js"
    ]
  },
  "dependencies": {
    "JSONPath": "0.11",
    "async": "2.6",
    "body-parser": "1.18",
    "bunyan": "1.8",
    "chalk": "2",
    "colors": "1",
    "compression": "1.7",
    "connect-flash": "0.1",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "1.4",
    "csv-stringify": "4.0",
    "express": "4.16",
    "express-handlebars": "3.1.0",
    "express-session": "1.15",
    "glob": "7.1",
    "handlebars": "^4.7.6",
    "helmet": "3.21.1",
    "lodash": "4.17.19",
    "method-override": "3.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.7",
    "mongoose-unique-validator": "2.0",
    "morgan": "1.9",
    "multipipe": "2.0",
    "nodemailer": "4.6",
    "passport": "0.4",
    "passport-local": "1.0",
    "path": "0.12",
    "platform": "1.3",
    "q": "1.5",
    "socket.io": "2.1",
    "socketio-sticky-session": "0.4",
    "swagger-jsdoc": "3.5.0",
    "swagger-parser": "~8.0.4",
    "swagger-ui-express": "4.1.3",
    "through2": "2.0",
    "uuid": "3.3"
  },
  "devDependencies": {
    "@types/express": "4.16",
    "@types/passport": "0.4",
    "@types/q": "1.5",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "kafka-node": "3.0",
    "lint-staged": "^10.0.7",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "mock-require": "3.0",
    "nodemon": "^2.0.3",
    "nyc": "14.1.1",
    "proxyquire": "2",
    "rewire": "^5.0.0",
    "should": "13",
    "sinon": "^7.5.0",
    "supertest": "3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
}
